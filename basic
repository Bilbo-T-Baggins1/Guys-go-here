# first input, question to the user

import webbrowser
# loop
var = 1
while var == 1:
    # first input, question to the user
    web = input ('Where you want to go ')

# options for the user
    if web == ('reddit'):
        webbrowser.open('https://www.reddit.com/')
        print ('enjoy the front page of the internet! p.s. I recommend 50-50 or watchpeopledie')

    elif web == ('youtube'):
        webbrowser.open ('https://www.youtube.com/')
        print ('Big Money Salvia Over and Out!')

    elif web == ('pornhub'):
        webbrowser.open ('https://www.pornhub.com/')
        print ('Cya in 5 minutes you cuck')

    elif web == ('facebook'):
        print ('are you fucking crazy? I do not want papa Zucc to spy in on us')

    elif web == ('myspace'):
        webbrowser.open ('https://myspace.com/')
        print ('Cringe lvl = 100')

    elif web == ('hackerrank'):
         webbroswer.open ('https://www.hackerrank.com/%27')
         print ('Gotta level up those scripts')
         
# error message for the user
    else:
        print ('nah nigga')
